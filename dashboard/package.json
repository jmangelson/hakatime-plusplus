{
  "name": "dashboard",
  "version": "1.0.0",
  "description": "Dashboard for Hakatime",
  "main": "index.js",
  "author": "Konstantinos Sideris <siderisk@auth.gr>",
  "license": "UNLICENSE",
  "private": true,
  "devDependencies": {
    "eslint": "7.9.0",
    "parcel": "^1.12.4",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-custom-dist-structure": "1.1.14",
    "parcel-plugin-inliner": "1.0.14",
    "parcel-plugin-static-files-copy": "2.5.0",
    "prettier": "2.1.2"
  },
  "scripts": {
    "dev": "parcel index.html --no-source-maps",
    "prod": "parcel build index.html --detailed-report --no-source-maps",
    "lint": "eslint --ext js src",
    "fmt": "prettier --write src/**/*.js src/*.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.14.0",
    "apexcharts": "3.22.0",
    "bootstrap": "4.5.2",
    "jquery": "3.5.1",
    "litepicker": "1.5.7",
    "lodash": "4.17.20",
    "mithril": "^2.0.4",
    "mithril-table": "^0.2.2",
    "polythene-mithril": "^1.7.4",
    "popper.js": "1.16.1",
    "startbootstrap-sb-admin-2": "4.1.1",
    "typeface-nunito": "1.1.3"
  },
  "staticFiles": {
    "staticPath": "icons"
  },
  "customDistStructure": {
    "options": {
      "development": true
    },
    "config": {
      ".css": "css",
      ".map": "css",
      ".js": "js",
      "images": [
        ".jpg",
        ".svg",
        ".png"
      ]
    }
  }
}
